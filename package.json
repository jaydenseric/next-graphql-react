  },
  "dependencies": {
    "http-link-header": "^1.0.3",
    "react-waterfall-render": "^2.0.0"
  },
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-env": "^22.0.0",
    "eslint-config-next": "^11.1.2",
